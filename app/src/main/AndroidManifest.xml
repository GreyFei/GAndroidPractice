<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.GAndroidPractice"
        tools:targetApi="31">
        <activity
            android:name=".MainActivity"
            android:exported="true"
            android:label="@string/app_name"
            android:theme="@style/Theme.GAndroidPractice">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <!-- region Package: activity -->
        <activity android:name="coder.greyfei.android.activity.PackageActivityEntryActivity" />
        <activity android:name="coder.greyfei.android.activity.ALActivityA" />
        <activity android:name="coder.greyfei.android.activity.ALActivityB" />
        <activity
            android:name="coder.greyfei.android.activity.ALActivityC"
            android:configChanges="orientation"
            />
        <activity
            android:name="coder.greyfei.android.activity.ALActivityD"
            android:configChanges="orientation|screenSize"
            />
        <activity android:name="coder.greyfei.android.activity.ALTestEntryActivity" />
        <!-- Activity启动模式 -->
        <activity android:name="coder.greyfei.android.activity.ALMTestEntryActivity" />
        <activity android:name="coder.greyfei.android.activity.ALMSingleInstanceTestEntryActivity" />
        <activity android:name="coder.greyfei.android.activity.ALMSingleInstanceTestActivity1" />
        <activity android:name="coder.greyfei.android.activity.ALMSingleInstanceTestActivity2"
            android:launchMode="singleInstance"
            />
        <activity android:name="coder.greyfei.android.activity.ALMSingleInstanceTestActivity3" />

        <!-- endregion -->

    </application>

</manifest>